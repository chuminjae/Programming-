#include <bits/stdc++.h>
using namespace std;
int cnt;
int k;
bool check(int a, int b){
        if(a > b){
            return true;
        }
        return false;

    }

int main(){
    int a, b;
    cin >> a >> b;
    int line[a][b];
    for(int i = 0; i < a; i++){
        for(int j = 0; j < b; j++){
            cin >> line[i][j];
        }
    }
        for(int i = 0; i < a; i++){
            for(int j = 0; j < b; j++){
                for(int k = 0; k < b; k++){
                     if(check(line[i][j], line[i][k])){
                        k++;
                        if(k == a){
                            cnt ++;
                        }
                     }

                }
                k = 0;
            }
        }
        cout << cnt;    



    }
