#include <bits/stdc++.h>
using namespace std;
int main(){
string a1, a2, a3;
int p;
stack <string> s;
cin >> p;
cin.ignore();
for(int i = 0; i < p; i++){
    getline(cin, a1);
    for(int j  = 0; j < a1.length(); j++){
        if(a1[j] = ' '){
            s.push(a2);
            s.push(" ");
            a2.clear();
        }
        else{
            a2 = a2 + a1[j];

        }
    }
    s.push(a2);
    while(!s.empty()){
        a3 = a3 + s.top();
        s.pop;
    }
    cout << a3;



}


}
