# include <bits/stdc++.h>
# include <math.h>
using namespace std;
int sort(v.begin(), v.end(), [](auto &left, auto &right) {
    return left.second < right.second;
});
int main(){
    int n,a;
    string b;
    cin >> n;
    vector <pair<string, int>> v;
    for(int i = 0; i < n; i++){
        cin >> {a,b};
    }
    sort(v.begin(), v.end());
    
}



